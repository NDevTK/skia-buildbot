load("@io_bazel_rules_go//go:def.bzl", "go_library")
load("//bazel/go:go_test.bzl", "go_test")

go_library(
    name = "perfresults",
    srcs = ["perf_results_parser.go"],
    importpath = "go.skia.org/infra/cabe/go/perfresults",
    visibility = ["//visibility:public"],
    deps = ["//go/skerr"],
)

go_test(
    name = "perfresults_test",
    srcs = ["perf_results_test.go"],
    data = glob(["testdata/**"]),
    embed = [":perfresults"],
    deps = ["@com_github_stretchr_testify//assert"],
)
